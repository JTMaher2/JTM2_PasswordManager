{
  "name": "jtm-jtm2-passwordsstenciljs",
  "version": "0.1.0",
  "private": true,
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/jtm-jtm2-passwordsstenciljs/jtm-jtm2-passwordsstenciljs.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "eslint --fix --ext .ts --ext .tsx --ext .js --ignore-pattern **/*.d.ts ./src && stencil build --docs",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "tsdoc": "tsc -d --emitDeclarationOnly --project ./ && api-extractor run --local && api-documenter markdown -i temp -o tempmd"
  },
  "devDependencies": {
    "@dnncommunity/dnn-elements": "^0.15.2",
    "@microsoft/api-documenter": "^7.23.23",
    "@microsoft/api-extractor": "^7.40.1",
    "@stencil/core": "^2.22.3",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@stencil/store": "^2.0.13",
    "@types/jquery": "3.5.25",
    "eslint-plugin-tsdoc": "^0.2.17"
  },
  "license": "MIT"
}
