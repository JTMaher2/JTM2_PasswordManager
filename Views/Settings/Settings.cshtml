@inherits DotNetNuke.Web.Mvc.Framework.DnnWebViewPage <JTMaher2.Modules.JTM2_PasswordManager.Models.Settings>

@using DotNetNuke.Web.Mvc.Helpers

<h2 id="dnnSitePanel-BasicSettings" class="dnnFormSectionHead"><a href="" class="dnnSectionExpanded">@Dnn.LocalizeString("BasicSettings")</a></h2>
<fieldset>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.MasterPassword, new { @class = "col-md-2 control-label" })
        </div>
        @Html.TextBoxFor(m => Model.MasterPassword)
    </div>
</fieldset>

